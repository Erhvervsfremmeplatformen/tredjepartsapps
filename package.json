{
  "name": "vg-applikation-test",
  "version": "0.1.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "eslint \"**/*.{ts,js,vue}\" --fix --format=pretty",
    "prettierfix": "prettier --write \"**/*.{vue,ts}\" ",
    "prettiercheck": "prettier --check \"**/*.{vue,ts}\" ",
    "test:unit": "vue-cli-service test:unit --coverage",
    "test:watch": "vue-cli-service test:unit --watchAll",
    "transpile-test": "cross-env VG_VUE_TRANSPILE=true vue-cli-service build --target lib --formats umd-min --dest build/transpiled --name=f957085aeab09f8d18000e98508c85b95655da84 src/components/Applikation.vue"
  },
  "dependencies": {
    "vue": "3.3.4",
    "dkfds": "8.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/eslint-plugin-tslint": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-typescript": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "compression-webpack-plugin": "4.0.1",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-formatter-pretty": "3.0.1",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-vue": "7.20.0",
    "prettier": "2.7.1",
    "sass": "1.55.0",
    "sass-loader": "10.3.1",
    "schema-dts": "1.1.0",
    "style-resources-loader": "1.5.0",
    "typescript": "4.5.5",
    "vue-cli-plugin-style-resources-loader": "0.1.5",
    "vue-eslint-parser": "7.11.0",
    "webpack": "5.76.0",
    "webpack-bundle-analyzer": "4.6.1",
    "@babel/traverse": "7.23.2",
    "postcss": "8.4.31",
    "jest-canvas-mock": "2.4.0",
    "@types/jest": "27.5.2",
    "@vue/cli-plugin-unit-jest": "5.0.8",
    "@vue/test-utils": "2.0.2",
    "@vue/vue3-jest": "27.0.0",
    "babel-jest": "27.5.1",
    "eslint-plugin-jest": "27.0.4",
    "jest": "27.5.1",
    "ts-jest": "27.1.5",
    "vue-typescript-jest": "0.3.1"
  },
  "overrides": {
    "node-ipc@>9.2.1 <10": "9.2.1",
    "node-ipc@>10.1.0": "10.1.0"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [],
    "coverageDirectory": "build/reports/test/html/jest/",
    "moduleFileExtensions": [
      "js",
      "vue",
      "ts"
    ],
    "transform": {
      "^.+\\.vue$": "@vue/vue3-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest",
      ".*\\.(ts|vue)$": "<rootDir>/node_modules/vue-typescript-jest/preprocessor.js",
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^~/(.*)$": "<rootDir>/tests/$1"
    },
    "testMatch": [
      "**/tests/unit/**/*.spec.ts"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "isolatedModules": true
      }
    },
    "testEnvironment": "jsdom",
    "testRunner": "jest-jasmine2"
  }
}
