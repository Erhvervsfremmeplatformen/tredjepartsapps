{
  "name": "vg-applikation-test",
  "version": "0.1.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "eslint \"**/*.{ts,js,vue}\" --fix --format=pretty",
    "prettierfix": "prettier --write \"**/*.{vue,ts}\" ",
    "prettiercheck": "prettier --check \"**/*.{vue,ts}\" ",
    "test:unit": "vue-cli-service test:unit --coverage",
    "test:watch": "vue-cli-service test:unit --watchAll",
    "transpile-test": "cross-env VG_VUE_TRANSPILE=true vue-cli-service build --inline-vue --target lib --formats umd-min --dest build/transpiled --name=f957085aeab09f8d18000e98508c85b95655da84 src/components/Applikation.vue"
  },
  "dependencies": {
    "@erst-vg/piwik-event-wrapper": "1.0.1",
    "axios": "0.21.4",
    "core-js": "^3.23.3",
    "dkfds": "7.4.0",
    "runtime": "0.14.1",
    "vue": "2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.18.2",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-typescript": "^4.4.6",
    "@vue/cli-plugin-unit-jest": "4.4.3",
    "@vue/cli-service": "^4.4.6",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.0.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-lodash": "^3.3.4",
    "compression-webpack-plugin": "^4.0.0",
    "flush-promises": "1.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "lint-staged": "^10.4.2",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.14.1",
    "prettier": "^2.3.2",
    "sass-loader": "^10.0.0",
    "schema-dts": "^1.0.0",
    "style-resources-loader": "^1.3.3",
    "jest": "^26.0.0",
    "ts-jest": "^26.3.0",
    "typescript": "4.5.5",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-eslint-parser": "^7.1.0",
    "vue-jest": "^4.0.1",
    "vue-typescript-jest": "0.3.1",
    "vue-template-compiler": "2.6.11",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "string.prototype.replaceall": "^1.0.5"
  },
  "overrides": {
    "node-ipc@>9.2.1 <10": "9.2.1",
    "node-ipc@>10.1.0": "10.1.0"
  },
  "jest": {
    "verbose": true,
    "coverageDirectory": "build/reports/test/html/jest/",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/jest-setup.js",
      "<rootDir>/tests/mocks.js"
    ],
    "moduleFileExtensions": [
      "js",
      "vue",
      "ts"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^~/(.*)$": "<rootDir>/tests/$1"
    },
    "testMatch": [
      "**/tests/unit/**/*.spec.ts"
    ]
  }
}
